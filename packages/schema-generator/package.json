{
    "name": "@yamayuski/babylon-vrm-schema-generator",
    "type": "module",
    "version": "0.0.1-alpha.0",
    "description": "JSON-Schema to TypeScript definition for babylon-vrm",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/yamayuski/babylon-vrm.git"
    },
    "keywords": [
        "3D",
        "html5",
        "webgl",
        "webgpu",
        "babylon.js",
        "gltf",
        "vrm"
    ],
    "homepage": "https://github.com/yamayuski/babylon-vrm",
    "bugs": {
        "url": "https://github.com/yamayuski/babylon-vrm/issues"
    },
    "license": "MIT",
    "author": {
        "name": "Masaru Yamagishi",
        "email": "m.yamagishi90+git@gmail.com",
        "url": "https://cafe-capy.net"
    },
    "files": [
        "dist/**"
    ],
    "main": "dist/umd/babylon-vrm-schema-generator.umd.cjs",
    "module": "dist/esm/babylon-vrm-schema-generator.esm.js",
    "types": "dist/types/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/types/index.d.ts",
            "import": "./dist/esm/babylon-vrm-schema-generator.esm.js",
            "require": "./dist/cjs/babylon-vrm-schema-generator.umd.cjs"
        }
    },
    "devDependencies": {
        "tsx": "^4.21.0"
    },
    "scripts": {
        "test": "vitest",
        "test:coverage": "vitest run --coverage",
        "generate": "npx tsx src/index.ts && pnpm -w run lint:fix"
    }
}
