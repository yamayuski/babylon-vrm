{
    "name": "@yamayuski/babylon-vrm-viewer",
    "type": "module",
    "version": "0.0.1-alpha.0",
    "description": "VRM browser viewer for Babylon.js",
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/yamayuski/babylon-vrm.git"
    },
    "keywords": [
        "3D",
        "html5",
        "webgl",
        "webgpu",
        "babylon.js",
        "gltf",
        "vrm",
        "viewer"
    ],
    "homepage": "https://github.com/yamayuski/babylon-vrm",
    "bugs": {
        "url": "https://github.com/yamayuski/babylon-vrm/issues"
    },
    "license": "MIT",
    "author": {
        "name": "Masaru Yamagishi",
        "email": "m.yamagishi90+git@gmail.com",
        "url": "https://cafe-capy.net"
    },
    "files": [
        "dist/**"
    ],
    "main": "dist/umd/babylon-vrm-viewer.umd.cjs",
    "module": "dist/esm/babylon-vrm-viewer.esm.js",
    "types": "dist/types/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/types/index.d.ts",
            "import": "./dist/esm/babylon-vrm-viewer.esm.js",
            "require": "./dist/cjs/babylon-vrm-viewer.cjs"
        }
    },
    "dependencies": {
        "@babylonjs/core": "^8.24.0",
        "@babylonjs/loaders": "^8.24.0"
    },
    "scripts": {
        "test": "vitest",
        "test:coverage": "vitest run --coverage",
        "build": "rm -rf dist && vite build && tsc --declaration --emitDeclarationOnly --outDir dist/types"
    }
}
